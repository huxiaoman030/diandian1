    "# 实现一个栈stack,后进先出\n",
    "# Stack() 创建一个新的空栈\n",
    "# push(item) 添加一个新的元素item到栈顶\n",
    "# pop() 弹出栈顶元素\n",
    "# peek() 返回栈顶元素\n",
    "# is_empty() 判断栈是否为空\n",
    "# size() 返回栈的元素个数\n",
    "\n",
    "classStack(object):\n",
    "def __init__(self):\n",
    "self.items = []\n",
    "\n",
    "def push(self,item):\n",
    "self.items.append(item)\n",
    "\n",
    "def pop(self):\n",
    "self.items.pop()\n",
    "\n",
    "def peek(self):\n",
    "return self.items[len(self.items)-1]\n",
    "\n",
    "def in_empty(self):\n",
    "if self.items == []:\n",
    "   return True\n",
    "else:\n",
    "   return Flase\n",
    "\n",
    "def size(self):\n",
    "return len(self.items)\n"
   ]
